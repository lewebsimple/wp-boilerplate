{
    "name": "lewebsimple/wp-boilerplate",
    "description": "WordPress project boilerplate.",
    "version": "0.1.6",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "composer",
            "url": "https://satis.ledevsimple.ca"
        }
    ],
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": "wp core download --force --skip-content && mkdir -p wp-content/plugins wp-content/themes wp-content/uploads"
    },
    "require": {
        "wpackagist-plugin/loco-translate": "*",
        "wpackagist-plugin/redirection": "*",
        "wpackagist-plugin/safe-svg": "*",
        "wpackagist-plugin/user-switching": "*"
    }
}
