{
  "name": "lewebsimple/wp-boilerplate",
  "description": "WordPress project boilerplate.",
  "version": "0.1.4",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://satis.ledevsimple.ca"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": "wp core download --force --skip-content",
    "post-create-project-cmd": "wp core download --force --skip-content"
  },
  "require": {
    "lewebsimple/acf-polylang": "*",
    "lewebsimple/advanced-custom-fields-pro": "*",
    "lewebsimple/ithemes-security-pro": "*",
    "wpackagist-plugin/autodescription": "*",
    "wpackagist-plugin/disable-comments": "*",
    "wpackagist-plugin/loco-translate": "*",
    "wpackagist-plugin/safe-svg": "*",
    "wpackagist-plugin/user-switching": "*"
  }
}